    "@polymer/iron-overlay-behavior": "",
    "@polymer/iron-pages": "",
    "@polymer/iron-selector": "",
    "@polymer/paper-behaviors": "",
    "@polymer/paper-icon-button": "",
    "@polymer/paper-spinner": "",
    "@polymer/polymer": "",
    "@webcomponents/webcomponentsjs": ""
  },
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "",
    "gulp-replace": "",
    "polymer-cli": "",
    "prpl-server": "",
    "wct-browser-legacy": ""
  }
