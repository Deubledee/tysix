{
  "entrypoint": "src/index.html",
  "shell": "src/cms-login.js",
  "fragments": [],
  "sources": [
    "data/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "push-manifest.json"
  ],
  "builds": [
    {
      "name": "es6-noAMD",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}
